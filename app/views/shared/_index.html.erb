<div class="row">
  <div class="col-lg-12">
    <table class="table table-hover">
      <thead>
        <tr>
          <td>Ranking</td>
          <td>Name</td>
          <td>Upvote</td>
        </tr>
      </thead>
      <tbody>
        <% objects.each do |object| %>
        <tr>
          <td>Upvotes: <%= object.upvotes %></td>
          <td><%= link_to object.title, polymorphic_path(object) %></td>
          <td><%= button_to "Upvote", polymorphic_path([:upvote, object]), method: :post, class: "btn btn-default" %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <%= link_to "View All Media", root_path, class: "btn btn-default" %>
    <%= link_to "Add a #{objects.first.class}", new_polymorphic_path(objects.first.class), class: "btn btn-default" %>
  </div>
</div>
